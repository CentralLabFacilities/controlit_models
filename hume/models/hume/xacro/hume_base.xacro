    </inertial>
    <visual>
      <origin rpy="1.5702 0 0 " xyz="0 0.0181 0.450"/>
      <geometry name="knee_l_geom">
        <mesh filename="package://hume_meshes/UMB_SM_knee_GJ.STL"/>
