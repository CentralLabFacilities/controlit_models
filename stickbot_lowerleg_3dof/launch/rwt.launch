<launch>
    <!-- Load the robot description parameter.  Launch the robot state publisher, joint state publisher, and RViz.-->
    <include file="$(find stickbot_lowerleg_3dof)/launch/rviz.launch" />

    <!-- Start ROS Bridge Server -->
    <include file="$(find rosbridge_server)/launch/rosbridge_websocket.launch" />

    <!-- Start the tf2_web_republisher -->
    <node name="tf2_web_republisher" pkg="tf2_web_republisher" type="tf2_web_republisher" output="screen" />
</launch>
